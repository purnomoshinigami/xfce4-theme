style "spinbutton"	= "default"
{

  xthickness            			= 2
  ythickness            			= 1
  GtkWidget::interior_focus	= 1

  engine "pixmap"
  {
    image
    {
      function			= ARROW
    }

############################# UP ######################xx
    image
    {
      function			= BOX
      state 			= NORMAL
      detail			= "spinbutton_up"
