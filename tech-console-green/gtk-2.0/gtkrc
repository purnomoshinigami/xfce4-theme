style "list-header"
{
ythickness = 3
GtkTreeView::odd_row_color = "#555555"
GtkTreeView::even_row_color = "#022a07"

engine "pixmap"
{
image
{
function 	= BOX
state		= NORMAL
file 		= "ListHeaders/list_header.png"
border 	= {4,4,4,4}
stretch 	= TRUE
}

image
{
function 	= BOX
state		= PRELIGHT
file 		= "ListHeaders/list_header-prelight.png"
border 	= {4,4,4,4}
stretch 	= TRUE
}

image
{
function 	= BOX
state		= ACTIVE
file		= "ListHeaders/list_header-pressed.png"
border 	= {4,4,4,4}
stretch 	= TRUE
}
image
{
function 	= BOX
state		= SELECTED
file		= "ListHeaders/list_header-prelight.png"
border 	= {4,4,4,4}
stretch 	= TRUE
}
image
{
function 	= BOX
state		= INSENSITIVE
file		= "ListHeaders/list_header-insens.png"
border 	= {4,4,4,4}
stretch 	= TRUE
}
}
}

