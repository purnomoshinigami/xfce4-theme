		stretch		= FALSE
	}

	
	image
	{
		function	= BOX
		state		= INSENSITIVE
		file		= "Panel/panel-bg.png"
		border		= { 1 , 1 , 1 , 1}
		stretch		= FALSE
	}

	
	image
	{
		function	= BOX
		state		= PRELIGHT
		recolorable	= TRUE
		file		= "Menu-Menubar/menubar-item.png"
		border		= { 1 , 1 , 1 , 1}
		stretch		= TRUE
	}

}

}

widget_class "*Panel*MenuBar*" style "panelbar"
